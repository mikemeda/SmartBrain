{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": false
  },
  "rules": {
    "new-cap": 1,
    "no-undef": 2,
    "strict": [2, "global"],
    "comma-style": [1, "last"],
    "comma-spacing": [1, {"before": false, "after": true}],
    "camelcase": [2, {"properties": "never"}],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "curly":2,
    "eqeqeq": 2,
    "no-empty":1,
    "indent": [2,2],
    "semi":[2, "always"],
    "no-underscore-dangle": 0,
    "no-use-before-define": 2,
    "no-mixed-spaces-and-tabs":1,
    "no-unused-vars": 2,
    "eol-last": 0,
    "max-len": [2,85,2],
    "quotes": [2, "single"]
  }
}